# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key
SUPABASE_STORAGE_BUCKET=files

# Database
DATABASE_URL=postgresql://user:password@host:port/database

# JWT Configuration
